{
  "name": "cli-watch-dog",
  "version": "1.0.1",
  "main": "src/index.js",
  "license": "MIT",
  "scripts": {
    "start": "node src/index.js",
    "clean": "rimraf dist",
    "pkg": "npm run clean && pkg src/index.js --options expose-gc --output dist/cli-watch-dog-1.0.1.exe"
  },
  "dependencies": {
    "@types/node": "15.6.1",
    "pkg": "5.2.1"
  },
  "pkg": {
    "scripts": "build/**/*.js",
    "assets": "views/**/*",
    "targets": [
      "node12-win-x64"
    ]
  },
  "devDependencies": {
    "rimraf": "^3.0.2"
  }
}
